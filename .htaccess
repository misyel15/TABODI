
RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f# Enable URL rewriting
RewriteEngine On
RewriteBase /

# Redirect requests for ".php" URLs to clean URLs
RewriteCond %{THE_REQUEST} ^[A-Z]{3,}\s([^.]+)\.php [NC]
RewriteRule ^ %1 [R=301,L]

RewriteRule ^([^\.]+)$ $1.php [NC,L]
